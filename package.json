{
  "name": "orbit",
  "version": "0.0.1",
  "description": "",
  "homepage": ".",
  "engines": {
    "npm": ">=3"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "local": "concurrently -k -r -s first \"yarn run test:watch\" \"yarn run open:local\" \"yarn run lint:watch\"",
    "open:local": "babel-node tools/localServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "open:drone": "babel-node tools/droneServer.js",
    "lint": "esw webpack.* src tools --color",
    "lint:watch": "yarn run lint -- --watch",
    "prebuild": "yarn run lint && yarn run test",
    "build": "babel-node tools/build.js && yarn run open:dist",
    "build-drone": "babel-node tools/build-drone.js",
    "build-production": "babel-node tools/build-prod.js",
    "test": "jest",
    "test:drone": "jest --verbose --coverage --bail",
    "test:watch": "jest --updateSnapshot --watch --coverage",
    "test:update": "jest --updateSnapshot",
    "test:cache": "yarn run test --clearCache",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "yarn run test -- --coverage",
    "test:cover:CI": "yarn run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "storybook": "NODE_PATH=src start-storybook",
    "open:cover": "yarn run test:cover && open ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "cypress": "cypress open",
    "cypress:CI": "DEBUG=cypress:server:browsers:electron cypress run --record --key 86bac6f4-1bca-41b9-b716-356cf7c8fee6"
  },
  "author": "Astronomer",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.0.0",
    "@nivo/bar": "0.59.2",
    "@nivo/line": "0.59.3",
    "@nivo/pie": "0.58.0",
    "@nivo/stream": "0.59.0",
    "apollo-cache-inmemory": "1.2.5",
    "apollo-client": "2.3.5",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "1.0.8",
    "apollo-utilities": "1.0.17",
    "classnames": "2.2.6",
    "copy-to-clipboard": "3.0.8",
    "cypress": "3.4.1",
    "eventemitter3": "3.1.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "http-serve": "1.0.1",
    "http-server": "0.11.1",
