  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/cypress/",
      "/tools/",
      "/src/helpers/",
      "/src/modules/",
      "/src/instruments/analytics/",
      "/src/instruments/index",
      "/src/instruments/api/",
      "/src/instruments/context/",
      "/src/instruments/forms/",
      "/src/instruments/react/",
      "/src/instruments/graphs/"
    ],
    "testPathIgnorePatterns": [
      "/cypress/",
      "/tools/",
      "/src/helpers/",
      "/src/modules/",
      "/src/instruments/analytics/",
      "/src/instruments/index",
      "/src/instruments/api/",
      "/src/instruments/context/",
      "/src/instruments/forms/",
      "/src/instruments/react/",
      "/src/instruments/graphs/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/assetsTransformer.js"
    },
    "setupFiles": [
      "raf/polyfill",
      "./tools/enzymeTestAdapterSetup.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts|cypress)[/\\\\]"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "> 1%"
  ]
}
