{
  "name": "orbit",
  "version": "0.0.1",
  "description": "",
  "homepage": ".",
  "engines": {
    "npm": ">=3"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "local": "concurrently -k -r -s first \"npm run test:watch\" \"npm run open:local\" \"npm run lint:watch\"",
    "open:local": "babel-node tools/localServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "prebuild": "npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "build-production": "babel-node tools/build-prod.js",
    "test": "jest",
    "test:CI": "babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watch",
    "open:cover": "npm run test:cover && open ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "author": "Astronomer",
  "license": "MIT",
  "dependencies": {
    "apollo-cache-inmemory": "1.2.5",
    "apollo-client": "2.3.5",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "1.1.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "1.0.8",
    "apollo-utilities": "1.0.17",
    "classnames": "2.2.6",
    "copy-to-clipboard": "3.0.8",
    "eventemitter3": "3.1.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "lodash": "4.17.11",
    "prop-types": "15.6.2",
    "rc-slider": "8.6.1",
    "react": "^16.5.0",
    "react-apollo": "2.1.9",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-imported-component": "4.6.2",
    "react-router-dom": "4.3.1",
    "semver": "5.6.0",
    "subscriptions-transport-ws": "0.9.14",
    "tinygradient": "0.4.1"
  },
  "devDependencies": {
    "autoprefixer": "8.6.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "22.4.4",
    "babel-loader": "7.1.5",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "3.6.0",
    "coveralls": "^2.11.14",
    "css-loader": "^0.28.11",
    "enzyme": "^3.4.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-watch": "3.1.4",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "0.7.6",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "jest-cli": "^21.2.1",
    "jest-enzyme": "^4.0.1",
    "json-loader": "^0.5.4",
    "mini-css-extract-plugin": "0.4.1",
    "node-sass": "4.9.2",
    "optimize-css-assets-webpack-plugin": "5.0.0",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "2.1.6",
    "prettier": "1.12.1",
    "raf": "^3.3.2",
    "react-hot-loader": "4.3.4",
    "react-test-renderer": "16.4.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
